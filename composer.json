{
    "name": "sonata-project/admin-search-bundle",
    "type": "symfony-bundle",
    "description": "Implement Search Engine (ElasticSearch) inside Sonata Admin",
    "keywords": [
        "Admin",
        "search",
        "sonata"
    ],
    "homepage": "http://sonata-project.org/bundles/admin-search",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataAdminBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "friendsofsymfony/elastica-bundle": "^3.1",
        "pagerfanta/pagerfanta": "^1.0",
        "ruflin/elastica": "^2.1 || ^3.0",
        "sonata-project/admin-bundle": "^3.31",
        "symfony/config": "^2.8 || ^3.2 || ^4.0",
        "symfony/dependency-injection": "^2.8 || ^3.2 || ^4.0",
        "symfony/form": "^2.8 || ^3.2 || ^4.0",
        "symfony/http-kernel": "^2.8 || ^3.2 || ^4.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^3.1",
        "symfony/phpunit-bridge": "^4.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\AdminSearchBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\AdminSearchBundle\\Tests\\": "tests/"
        }
    }
}
