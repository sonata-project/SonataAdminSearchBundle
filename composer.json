{
    "name": "sonata-project/admin-search-bundle",
    "type": "symfony-bundle",
    "description": "Implement Search Engine (ElasticSearch) inside Sonata Admin",
    "keywords": ["Admin", "search", "sonata" ],
    "homepage": "http://sonata-project.org/bundles/admin-search",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataAdminBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "friendsofsymfony/elastica-bundle": "^3.1",
        "pagerfanta/pagerfanta": "^1.0",
        "ruflin/elastica": "^2.1 || ^3.0",
        "sonata-project/admin-bundle": "^3.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "sllh/php-cs-fixer-styleci-bridge": "^2.0",
        "symfony/phpunit-bridge": "^3.3"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "Sonata\\AdminSearchBundle\\": "" },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Sonata\\AdminSearchBundle\\Tests\\": "Tests/" }
     },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
